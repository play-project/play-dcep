<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE definition PUBLIC "-//objectweb.org//DTD Fractal ADL 2.0//EN" "classpath://org/objectweb/proactive/core/component/adl/xml/proactive.dtd">
<definition name="ConnectionWithEventCloud">

  <interface name="PublishApi" signature="fr.inria.eventcloud.api.PublishApi" role="server"/>
  <interface name="SubscribeApi" signature="fr.inria.eventcloud.api.SubscribeApi" role="server"/>

  <component name="EC" definition="EC"/>
  
  <binding client="this.SubscribeApi" server="EC.SubscribeApi"/>
  <binding client="this.PublishApi" server="EC.PublishApi"/>
  
   <controller desc="composite"/>
</definition>