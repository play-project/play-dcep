# Simple CONSTRUCT query.
PREFIX pl: 		<http://play-project.eu/bdpl> 
PREFIX rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX :       <http://events.event-processing.org/types/>

CONSTRUCT{
?x pl:nice ?name .
:e rdf:type :Temperature .
}
 WHERE {
	EVENT ?id{ ?e1 rdf:type :Temperature .
   		?e1 :stream <http://streams.event-processing.org/ids/Temperature#stream> .
		?x pl:nice ?name} 
	SEQ
	EVENT ?id{ ?e2 rdf:type :Temperature .
   		?e2 :stream <http://streams.event-processing.org/ids/Temperature#stream> .
		?x pl:nice ?name}
}
	