#Test mixture of GRAPH and EVENT 
PREFIX dc:	<http://play-project.eu/bdpl>
PREFIX :	<http://events.event-processing.org/types/>
PREFIX rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#>

CONSTRUCT {
	?e2 ?x ?mbox .
	:e rdf:type :MailboxEvent .
}
WHERE {
	EVENT ?e1 { ?e1 rdf:type :Temperature .
		?e1 :stream <http://streams.event-processing.org/ids/Temperature#stream> .
		?x dc:mbox ?mbox}
	SEQ 
	EVENT ?e2 { ?e2 rdf:type :Temperature .
		?e2 :stream <http://streams.event-processing.org/ids/Temperature#stream> .
		?x dc:mbox ?mbox }
	GRAPH ?e3 { ?e3 rdf:type :Temperature .
		?e3 :stream <http://streams.event-processing.org/ids/Temperature#stream> .
		?x dc:mbox ?mbox }
}