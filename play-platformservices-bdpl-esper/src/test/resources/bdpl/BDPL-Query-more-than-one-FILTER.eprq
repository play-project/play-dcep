# Too many variables. @expectedException<Only one Filter expression allowed>
PREFIX dc: <http://play-project.eu/bdpl> 
PREFIX rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX m12: <http://play-project.eu/M12/>
PREFIX :       <http://events.event-processing.org/types/>


	CONSTRUCT { 
				:e rdf:type :FaceBookComplexEvent.
				?id1 ?id2 ?id3 
			} 
		WHERE{
			WINDOW{
				EVENT ?id1{ 
							?e1 rdf:type :FacebookStatusFeedEvent.
							?e1 :stream <http://streams.event-processing.org/ids/FacebookStatus#stream> .
							m12:source m12:is m12:myFackebookAccountID.
							?friend1 m12:talksAbout ?topic.
							FILTER contains(?topic, "Jeans")
							}
							
                             SEQ
				EVENT ?id2{ 
							 ?e2 rdf:type :FacebookStatusFeedEvent.
							?e2 :stream <http://streams.event-processing.org/ids/FacebookStatus#stream> .
							m12:source m12:is m12:myFackebookAccountID.
							?friend2 m12:talksAbout ?topic.
							FILTER contains(?topic, "Jeans")
							}
							
                             SEQ
				EVENT ?id3{ 
							 ?e3 rdf:type :FacebookStatusFeedEvent.
							?e3 :stream <http://streams.event-processing.org/ids/FacebookStatus#stream> .
							m12:source m12:is m12:myFackebookAccountID.
							?friend3 m12:talksAbout ?topic
							}
         } ("PT30M"^^xsd:duration, sliding)
     }
